using asd
using morfae
using TeamWork
using SuelaWholesomeness

setup
{
number i := 1
number j := 2
text t := "This is a string"
bool b := true

lightbulb lb := pin 1
coffeeMachine cm := pin 2,3,4
temperatureSensor ts := pin 5

group numAndCoffee{
	i
	j
	cm
}
group everything{
	i
	j
	t
	b
	cm
	lb
	ts
	numAndCoffee
}
list number lstNum {}

}

repeat
{

}